{
    "$schema": "http://json-schema.org/schema",
    "$id": "SchemaToTypescript",
    "title": "Generates Typescript types from a json-schema file",
    "type": "object",
    "properties": {
        "schema": {
            "type": "string",
            "description": "",
            "$default": {
                "$source": "argv",
                "index": 0
            },
            "x-prompt": "Where is the schema file located?",
            "x-completion-type": "file",
            "x-completion-glob": "*.json"
        }
    },
    "required": ["schema"]
}
